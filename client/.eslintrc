{
  "extends": "airbnb",
  "rules": {
    "no-tabs": 0,
    "prefer-arrow-callback": 1,
    "semi": 1,
    "strict": 0,
    "indent": [2, "tab"],
    "quotes": [1, "single"],
    "no-multi-spaces": [1, {
      "exceptions": {
      "VariableDeclarator": true,
      "FunctionExpression": true
     }
    }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "key-spacing": [0, {"align": "value"}],
    "no-underscore-dangle": 0
  }
}
